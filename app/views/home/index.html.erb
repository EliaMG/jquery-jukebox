<div class="container-fluid">
  <div class="col-md-5 col-md-offset-4">
    <h1 class="eighties">Search for your Jam!</h1>
    <div class="other-api">
      <%= button_tag "IDK, Randomize!", class: "random" %>
      <%= button_tag "Show me what's HOT!", class: "popular" %>
    </div>
    <div class=search-form>
      <%= form_tag(search_path, method: :get, role: "search") do %>
        <%= label_tag "Search: " %>
        <%= text_field_tag :artist, nil, placeholder: "by Artist" %>
        <%= button_tag "Find me a JAM", class: "submit" %>
      <% end %>
    </div>
    <div class="results"></div>
  </div>
</div>
